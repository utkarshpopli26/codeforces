#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int mod = 1e9+7;
 
void solve(){
    int n,s;
    cin>>n>>s;
 
	int val = s/n;
 
	int rem = s%n;
 
	int val_2 = rem + val;
 
	if(val>1){
 
		cout<<"YES\n";
 
		for(int i=0 ; i<n-1 ; i++){
			cout<<val<<" ";
		}cout<<val_2<<endl;
 
		cout<<1<<endl;
	}
 
	else cout<<"NO\n";
 
}
 
int main(){
 
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
	
        solve();
 
    return 0;
 }
